{
  "name": "kye-vts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:vts-app-h5": "pnpm -C packages/web-vts-app-h5 dev",
    "dev:vts-bq-h5": "pnpm -C packages/web-vts-bq-h5 dev",
    "dev:vts-h5": "pnpm -C packages/web-vts-h5 dev",
    "dev:vts-ks-cargo": "pnpm -C packages/web-vts-ks-cargo dev",
    "dev:vts-ks-report": "pnpm -C packages/web-vts-ks-report dev",
    "dev:vts-market-h5": "pnpm -C packages/web-vts-market-h5 dev",
    "dev:vts-ks-market-vehicle": "pnpm -C packages/vts-ks-market-vehicle dev",
    "dev:docs": "pnpm -C docs dev",
    "dev:demo": "pnpm -C packages/web-demo dev",
    "dev": "run-p dev:vts-app-h5 dev:vts-h5",
    "build:vts-app-h5": "pnpm -C packages/web-vts-app-h5 build-only",
    "build:vts-bq-h5": "pnpm -C packages/web-vts-bq-h5 build",
    "build:vts-h5": "pnpm -C packages/web-vts-h5 build",
    "build:vts-ks-cargo": "pnpm -C packages/vts-ks-cargo build",
    "build:vts-ks-report": "pnpm -C packages/vts-ks-report build",
    "build:docs": "pnpm -C docs build",
    "build:vite-config": "pnpm -C packages/vite-config build",
    "build:map": "pnpm -C packages/map build",
    "build:captcha": "pnpm -C packages/captcha build",
    "build:demo": "pnpm -C packages/web-demo build",
    "build": "run-p  build:vts-app-h5 build:vts-h5",
    "git:add": "node ./scripts/gitSubAdd.js",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "lint": "eslint packages/web-biz --ext .js --ext .ts --ext .tsx",
    "fix": "eslint --fix  packages/web-biz  --ext .ts --ext .tsx",
    "publish:monitor": "pnpm -C packages/monitor publishX",
    "publish:shared": "pnpm -C packages/shared publishX",
    "publish:js-bridge": "pnpm -C packages/shared publishX",
    "publish:eslint-config": "pnpm -C packages/eslint-config publishX",
    "publish:vite-config": "pnpm -C packages/vite-config publishX",
    "publish:tsconfig": "pnpm -C packages/tsconfig publishX",
    "publish:map": "pnpm -C packages/map publishX",
    "publish:captcha": "pnpm -C packages/captcha publishX",
    "publish:http-client": "pnpm -C packages/http-client publishX",
    "publish:h5-ui": "pnpm -C packages/ui publishX"
  },
  "lint-staged": {
    "packages/web-biz/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^1.6.7",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "18",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.15",
    "commitizen": "^4.3.0",
    "consola": "^2.15.3",
    "core-js": "^3.36.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "fs-extra": "^11.2.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.76.0",
    "typescript": "^5.4.4",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.8.1",
    "vitest": "^1.2.2",
    "vue-tsc": "^2.0.10"
  }
}
